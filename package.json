{
    "name": "react-parcel-boilerplate",
    "version": "1.0.0",
    "description": "React Parcel Boilerplate",
    "author": "Matt Lockyer",
    "license": "ISC",
    "homepage": "https://myusername.github.io/my-app",
    "dependencies": {
        "animejs": "^3.2.1",
        "copy-to-clipboard": "^3.3.1",
        "near-api-js": "^0.44.2",
        "near-seed-phrase": "^0.2.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-react": "^7.12.10",
        "babel-plugin-react-html-attrs": "^3.0.5",
        "eslint": "^7.14.0",
        "gh-pages": "^3.2.3",
        "parcel": "^2.0.0-beta.1",
        "rimraf": "^3.0.2"
    },
    "browserslist": [
        "last 2 Chrome versions"
    ],
    "scripts": {
        "build": "parcel build  src/index.html --no-source-maps --public-url https://github.com/Dmytro-Shashkevych/craftoriumwallet.github.io",
        "clean": "rimraf dist/index.html dist/src.*.css dist/src.*.js dist/src.*.map",
        "predeploy": "npm run clean -s && npm run build -s",
        "deploy": "gh-pages -d dist",
        "prebuild": "yarn clean",
        "fix": "eslint src/ --fix",
        "start": "yarn prebuild && parcel src/index.html --open"
    }
}
