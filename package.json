{
    "name": "react-parcel-boilerplate",
    "version": "1.0.0",
    "description": "React Parcel Boilerplate",
    "author": "Matt Lockyer",
    "license": "ISC",
    "homepage": "https://github.com/Dmytro-Shashkevych/near-wallet-generator",
    "dependencies": {
        "animejs": "^3.2.1",
        "copy-to-clipboard": "^3.3.1",
        "near-api-js": "^0.44.2",
        "near-seed-phrase": "^0.2.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-react": "^7.12.10",
        "babel-plugin-react-html-attrs": "^3.0.5",
        "eslint": "^7.14.0",
        "gh-pages": "^3.2.3",
        "gulp": "^4.0.2",
        "gulp-gh-pages": "^0.5.4",
        "parcel": "^2.0.0-beta.1",
        "rimraf": "^3.0.2"
    },
    "scripts": {
        "prebuild": "yarn clean",
        "build": "parcel build  src/index.html --no-source-maps",
        "clean": "rimraf ./dist && rimraf .parcel-cache",
        "fix": "eslint src/ --fix",
        "start": "yarn prebuild && parcel src/index.html --open",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist"
    }
}
